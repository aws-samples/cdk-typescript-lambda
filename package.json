{
  "name": "cdk-ts-lambda",
  "version": "0.1.0",
  "bin": {
    "cdk-ts-lambda": "bin/cdk-ts-lambda.js"
  },
  "scripts": {
    "build": "tsc && npm run build-src",
    "build-src": "cd src && npm install && npm run build",
    "package": "cd src && npm run package",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "format": "prettier --write '{**/*.ts, **/**/*.ts}'",
    "lint": "eslint ./**/*.ts --fix"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.98.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.4.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-iam": "^1.98.0",
    "@aws-cdk/aws-lambda": "^1.98.0",
    "@aws-cdk/core": "^1.98.0",
    "aws-cdk": "^1.98.0",
    "source-map-support": "^0.5.16"
  }
}
